{
    "name": "SuperPasteURL",
    "displayName": "SuperPasteURL",
    "description": "Paste URL in Markdown format",
    "version": "0.0.1",
    "publisher": "alincode",
    "author": "alincode <alincode@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/alincode/SuperPasteURL.git"
    },
    "bugs": {
        "url": "https://github.com/alincode/SuperPasteURL/issues"
    },
    "homepage": "https://github.com/alincode/SuperPasteURL#readme",
    "keywords": [
        "vscode",
        "extension"
    ],
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.SuperPasteURL"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.SuperPasteURL",
                "title": "Paste URL"
            }
        ],
        "keybindings": [
            {
                "key": "ctrl+alt+p",
                "command": "extension.SuperPasteURL"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "copy-paste": "^1.3.0",
        "get-title": "^1.0.1",
        "get-title-at-url": "^1.1.6",
        "request": "^2.88.0",
        "request-promise": "^4.2.2"
    }
}
