{
    "name": "AwesomePasteURL",
    "displayName": "AwesomePasteURL",
    "description": "Paste URL in Markdown format",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.AwesomePasteURL"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.AwesomePasteURL",
                "title": "Paste URL"
            }
        ],
        "keybindings": [
            {
                "key": "ctrl+alt+p",
                "command": "extension.AwesomePasteURL"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "copy-paste": "^1.3.0",
        "get-title": "^1.0.1",
        "hyperquest": "^2.1.3"
    }
}
